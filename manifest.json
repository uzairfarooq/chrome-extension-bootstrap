{
  "manifest_version": 2,

  "name": "Sample",
  "description": "",
  "version": "1.0",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "browser_action": {
    "default_icon": {
      "19": "assets/icon19.png",
      "38": "assets/icon38.png"
    },
    "default_title": "Sample",
    "default_popup": "popup/popup.html"
  },

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["*://www.google.com/*"],
      "css": ["contentStyle.css"],
      "js": ["libs/jquery.min.js", "contentScript.js"]
    }
  ],

  "permissions": [

  ]
}